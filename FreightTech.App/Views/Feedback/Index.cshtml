@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    string baseUrl = Request.Url.AbsoluteUri.ToLowerInvariant().Contains("localhost") ? "" : "/app";
}

<div id="main-content">

    <div id="div-main" class="container-fluid">
        <div class="dashboard-section">
            <div class="section-heading clearfix">
                <h2 class="section-title">FeedBack</h2>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Name</th>

                                <th>Messages</th>

                            </tr>
                        </thead>
                        <tbody>
                            <!-- ko foreach:FeedbackArray -->
                            <tr>
                                <td data-bind="text: user.FirstName +' '+user.LastName"></td>

                                <td data-bind="text: detail.Feedbacks"></td>

                            </tr>
                            <!-- /ko -->
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>

</div>


<script type="text/javascript" src="@baseUrl/Scripts/libs/requirejs/require.js"
        data-main="@baseUrl/Scripts/app/modules/Feedback.js"></script>

